{% extends "FOSUserBundle::layoutconfirmed.html.twig" %}

{% trans_default_domain 'FOSUserBundle' %}

{% block fos_user_content %}
    <p style="color:#edde34">{{ 'registration.confirmed'|trans({'%username%': user.username}) }}</p>
    {% if targetUrl %}
    <p><a href="{{ targetUrl }}">{{ 'registration.back'|trans }}</a></p>
    {% endif %}
  <center> <a href="{{ path('fos_user_security_login') }}"> cliquer ici </a></center>
{% endblock fos_user_content %}
